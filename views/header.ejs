<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles.css">
    <title>Nude Inventory</title>
</head>
<body>
    <header>
        <nav>
            <div>
                <a href="/">Home</a>
            </div>
            <ul>
                <% categories.forEach(category => { %>
                    <li><a href="/category/<%= category.categorie %>"><%= category.categorie %></a></li>
                <% }) %>
            </ul>
        </nav>
    </header>
</body>
</html>